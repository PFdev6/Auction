<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @main_news.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= raw @main_news.description %>
</p>
<% if can? :create, @main_news%>
  <%= link_to 'Edit', edit_main_news_path(@main_news), class: "badge badge-dark" %> 
  <%= link_to 'Back', main_news_index_path, class: "badge badge-dark" %>
<% end %>