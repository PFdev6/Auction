<div class="m-3 jumbotron jumbotron-fluid">
  <div class="container">
    <% if @tag.nil? %>
        <p class="lead"> No lots with this tag </p>
    <% else %>
        <h2 class="display-3"> Tag: <%= @tag.name %> </h2>
        <div class="card-columns">
         <%= render @tag.lots %>
        <div>
    <%end%>
  </div>
</div>
